\n                        <div>\r\n                            <span>Bem-vindo,</span>\r\n                            <strong>{user.name}</strong>\r\n                        </div>\r\n                    </Profile>\r\n\r\n                    <button type=\"button\" onClick={signOut}>\r\n                        <FiPower />\r\n                    </button>\r\n                </HeaderContent>\r\n            </Header>\r\n\r\n            <Content>\r\n                <Schedule>\r\n                    <h1>Horários agendados</h1>\r\n                    <p>\r\n                        {isToday(selectedDate) && (<span>Hoje</span>)}\r\n                        <span>{selectedDateAsText}</span>\r\n                        <span>{selectedWeekDay}</span>\r\n                    </p>\r\n                    <NextAppointment>\r\n                        <strong>Atendimento(os) a seguir</strong>\r\n                        <div>\r\n                            <img\r\n                                src=\"https://avatars.githubusercontent.com/u/26776218?s=460&u=964b019741d54bdcd2d147c5dd86c4d495bf76dd&v=4\"\r\n                                alt=\"Avatar\"\r\n                            />\r\n                            <strong>André Cavalcanti</strong>\r\n                            <span>\r\n                                <FiClock />\r\n                                10:00\r\n                        </span>\r\n                        </div>\r\n                    </NextAppointment>\r\n\r\n                    <Section>\r\n                        <strong>Manhã</strong>\r\n                        <Appointment>\r\n                            <span>\r\n                                <FiClock />\r\n                                10:00\r\n                            </span>\r\n\r\n                            <div>\r\n                                <img\r\n                                    src=\"https://avatars.githubusercontent.com/u/26776218?s=460&u=964b019741d54bdcd2d147c5dd86c4d495bf76dd&v=4\"\r\n                                    alt=\"Avatar\"\r\n                                />\r\n                                <strong>André Cavalcanti</strong>\r\n                            </div>\r\n                        </Appointment>\r\n                        <Appointment>\r\n                            <span>\r\n                                <FiClock />\r\n                                10:00\r\n                            </span>\r\n\r\n                            <div>\r\n                                <img\r\n                                    src=\"https://avatars.githubusercontent.com/u/26776218?s=460&u=964b019741d54bdcd2d147c5dd86c4d495bf76dd&v=4\"\r\n                                    alt=\"Avatar\"\r\n                                />\r\n                                <strong>André Cavalcanti</strong>\r\n                            </div>\r\n                        </Appointment>\r\n                    </Section>\r\n\r\n                    <Section>\r\n                        <strong>Tarde</strong>\r\n                        <Appointment>\r\n                            <span>\r\n                                <FiClock />\r\n                                10:00\r\n                            </span>\r\n\r\n                            <div>\r\n                                <img\r\n                                    src=\"https://avatars.githubusercontent.com/u/26776218?s=460&u=964b019741d54bdcd2d147c5dd86c4d495bf76dd&v=4\"\r\n                                    alt=\"Avatar\"\r\n                                />\r\n                                <strong>André Cavalcanti</strong>\r\n                            </div>\r\n                        </Appointment>\r\n                    </Section>\r\n                </Schedule>\r\n                <Calendar>\r\n                    <DayPicker\r\n                        weekdaysShort={['D', 'S', 'T', 'Q', 'Q', 'S', 'S']}\r\n                        fromMonth={new Date()}\r\n              